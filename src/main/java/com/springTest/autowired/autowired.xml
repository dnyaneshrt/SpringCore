<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.1.xsd">

<context:annotation-config />
<bean name="marks1" class="com.springTest.autowired.Marks">
	<property name="math_marks" value="85"></property>
	<property name="eng_marks" value="99"></property>
</bean>
<bean name="marks2" class="com.springTest.autowired.Marks">
	<property name="math_marks" value="34"></property>
	<property name="eng_marks" value="29"></property>
</bean>


<bean name="Student" class="com.springTest.autowired.Student">
	<property  name="id" value="111"></property>
</bean>

</beans>